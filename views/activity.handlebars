<div class="tabbed-container">
    <div class="row">
        <div class="col s12">
            <div class="page-title">My Activity</div>
        </div>
    </div>
    <div class="row">
        <div class="col s12">
            {{#each activityData}}
            <div id="match-{{id}}" class="card">
                <div class="card-content">
                    <span class="card-title">{{yelp_business_name}}</span>
                    <p>Matched on {{matched_date}}</p>
                </div>
                <div class="card-action right-align">
                    <a href="https://www.yelp.com/map/{{yelp_business_id}}" class="btn waves-effect waves-light" target="_blank">Map it!</a>
                    <button data-id="{{id}}" class="btn waves-effect waves-light red remove-match">Remove</button>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>